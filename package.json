{
  "name": "stoat-for-web",
  "version": "0.0.14",
  "version-date": "2025-10-10",
  "description": "Stoat for Web: frontend software for Stoat",
  "scripts": {
    "dev:web": "pnpm --filter client dev",
    "build:deps": "pnpm --filter stoat.js build && pnpm --filter solid-livekit-components build && pnpm --filter @lingui-solid/babel-plugin-lingui-macro build && pnpm --filter @lingui-solid/babel-plugin-extract-messages build && pnpm --filter client lingui:compile",
    "build:web": "pnpm --filter client build",
    "build:hosted": "pnpm --filter client build:hosted",
    "start:web": "cd packages/client && pnpm run serve",
    "lint": "eslint --fix --ext .ts,.tsx packages/client",
    "fmt": "prettier --write 'packages/client/**/*.{ts,tsx,json}'",
    "lingui": "pnpm --filter client lingui:compile && pnpm --filter client lingui:extract",
    "test:lint": "eslint --ext .ts,.tsx packages/client",
    "test:fmt": "prettier --check 'packages/client/**/*.{ts,tsx,json}'",
    "test:typecheck": "pnpm --filter client test:typecheck",
    "test:suite": "pnpm --filter client test:suite",
    "test:coverage": "pnpm --filter client test:coverage",
    "test:e2e": "pnpm --filter client test:e2e",
    "test:e2e:report": "pnpm --filter client test:e2e:report"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@pandabox/prettier-plugin": "^0.1.3",
    "eslint": "^9.26.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-solid": "^0.14.5",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript-eslint": "^8.32.0"
  },
  "keywords": [],
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "private": true,
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
